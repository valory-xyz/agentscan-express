# App Configuration
PORT=4000
NODE_ENV=development
CLIENT_URL=http://localhost:3000

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Database Configuration - Production
POSTGRES_URL="postgresql://postgres:your_password@your_host:5432/your_database"
POSTGRES_USER=postgres
POSTGRES_HOST=your_host
POSTGRES_DB=your_database
POSTGRES_PASSWORD=your_password
POSTGRES_PORT=5432
POSTGRES_SSL=true

# Database Configuration - Local Development
LOCAL_POSTGRES_URL="postgresql://postgres:postgres@localhost:5432/agentscan_local"
LOCAL_POSTGRES_USER=postgres
LOCAL_POSTGRES_HOST=localhost
LOCAL_POSTGRES_DB=agentscan_local
LOCAL_POSTGRES_PASSWORD=postgres
LOCAL_POSTGRES_PORT=5432
LOCAL_POSTGRES_SSL=false
USE_LOCAL_DB=true

# API Keys and Secrets (replace with your own values)
ANTHROPIC_API_KEY="your_anthropic_api_key"
OPEN_API_KEY="your_openai_api_key"
GITHUB_ACCESS_TOKEN="your_github_token"
YOUTUBE_API_KEY="your_youtube_api_key"
AMPLITUDE_API_KEY="your_amplitude_api_key"
REPLICATE_API_TOKEN="your_replicate_token"

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
S3_BUCKET_NAME=your_bucket_name

# Authentication
PRIVY_APP_ID="your_privy_app_id"
PRIVY_APP_SECRET="your_privy_app_secret"

# Bot Tokens
DISCORD_BOT_TOKEN="your_discord_bot_token"
TELEGRAM_BOT_TOKEN="your_telegram_bot_token"


# OLAS Database (if needed)
OLAS_DB_USER=postgres
OLAS_DB_PASSWORD=your_olas_password
